{
  "name": "rs-csv2json",
  "version": "1.1.1",
  "description": "Converts CSV files to JSON files",
  "keywords": [
    "csv",
    "csv parser",
    "parse csv",
    "csvtojson",
    "json",
    "csv to json",
    "csv convert",
    "tojson",
    "convert csv to json",
    "csv-json"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "start": "npm run bundle && node ./src/demo.js",
    "bundle": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "bundle:esm": "tsc -p tsconfig.json",
    "bundle:cjs": "tsc -p tsconfig-cjs.json",
    "prepublish": "npm run bundle",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsteinmann/rs-csv2json.git"
  },
  "author": "steinmann.raphael@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rsteinmann/rs-csv2json/issues"
  },
  "homepage": "https://github.com/rsteinmann/rs-csv2json#readme",
  "dependencies": {
    "@types/node": "^18.0.4",
    "csvtojson": "^2.0.10",
    "typescript": "^4.7.4"
  }
}
